    "version": "0.2.0",
    "configurations": [
        {
            "name": "SFCoreProTM Presentation",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/SFCoreProTM/SFCoreProTM.Presentation/bin/Debug/net10.0/SFCoreProTM.Presentation.dll",
            "args": [],
            "cwd": "${workspaceFolder}/SFCoreProTM/SFCoreProTM.Presentation",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/SFCoreProTM/SFCoreProTM.Presentation/Views"
            }
        },
